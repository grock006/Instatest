<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Street View containers</title>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDyH6_rxueEsjg0nKXAN23lhziVfY1rirU"></script>
    <script>
      function initialize() {
        var map = new google.maps.LatLng(<%= @lat %>,<%= @lng %>);
        

        var panoramaOptions = {
          position: map,
          pov: {
            heading: 0,
            pitch: 0
          },
          zoom: 0,
          linksControl: true,
          panControl: true,
          zoomControlOptions: {
              style: google.maps.ZoomControlStyle.SMALL
            },
          enableCloseButton: true,
        };
        
        var myPano = new google.maps.StreetViewPanorama(
            document.getElementById('map-canvas'),
            panoramaOptions);
        myPano.setVisible(true);
      }

      google.maps.event.addDomListener(window, 'load', initialize);
    </script>
  </head>
  <body>
  	

    <div class="panel-1">
      <div class="outline">
        <i class="fa fa-instagram fa-lg" style="float:right; padding: 5px;"></i>
        <div class="inner">
        <a href="<%= @image_one.link %>" target="_blank" title="#"><img class="main" src="<%= @image_one.images.thumbnail.url %>">
          <h4>
            <img class="profile" src="<%= @image_one.user.profile_picture %>">  
            <%= @image_one.user.username%>
          </h4>
        </a>
      </div>
      </div>
      <button class="btn btn-default btn-sm" style="float:right;"><i class="fa fa-star-o"></i></button>
    </div>

    <div class="panel-2">
      <i class="fa fa-instagram fa-lg" style="float:right; padding: 5px;"></i>
        <a href="<%= @image_two.link %>" target="_blank" title="#">
          <img src="<%= @image_two.images.thumbnail.url %>" class="main">       
        <h4>
          <img src="<%= @image_one.user.profile_picture %>" class="profile">  
          <%= @image_one.user.username%>
        </h4>
        </a> 
        <button class="btn btn-default btn-sm" style="float:right;">Save <i class="fa fa-star-o"></i></button>
     </div>

 
<!--     <div class="panel-3">
        <a href="<%= @image_three.link %>" target="_blank" title="#"><img src="<%= @image_three.images.thumbnail.url %>" style="width:100%"></a>
    </div> -->

    <div class="panel-4">
      <div class="outline">
           <i class="fa fa-instagram fa-lg" style="float:right; padding: 5px;"></i>
        <div class="inner">
        <a href="<%= @image_four.link %>" target="_blank" title="#"><img src="<%= @image_four.images.thumbnail.url %>" style="width:100%">
            <h4>
            <img class="profile" src="<%= @image_four.user.profile_picture %>">  
            <%= @image_one.user.username%>
          </h4>
        </a>
      </div>
    </div>
    </div>

    <div class="panel-5">
        <a href="<%= @image_five.link %>" target="_blank" title="#"><img src="<%= @image_five.images.thumbnail.url %>" style="width:100%"></a>
     </div>
    
    <div class="panel-6">
        <a href="<%= @image_six.link %>" target="_blank" title="#"><img src="<%= @image_six.images.thumbnail.url %>" style="width:100%"></a>
     </div>
    
    <!-- Tweets -->

    <% @tweet_two.each do |t| %>
      <% unless t.text == nil%>
     <div class="panel-7">
        <ul class="media-list">
            <li class="media">
              <a class="media-left" href="#" title="#">
                <img src="<%= t.user.profile_image_url %>" alt="">
              </a>
              <div class="media-body">
                
                <h5 class="media-heading"><%= t.user.name %>      <i class="fa fa-twitter fa-lg"></i></h5>
                
                <h5> <a href="http://www.twitter.com/<%= t.user.screen_name %>">
                @<%= t.user.screen_name %></a></h5>
                
                <p>
                  <a href="http://www.twitter.com/<%= t.user.screen_name %>/status/<%= t.id %>">
                  <%= t.text %>
                   </a>
                </p>
              </div>
            </li>
          </ul>
     </div>
     <% end %>
     <% end %>




     <div class="panel-8">
